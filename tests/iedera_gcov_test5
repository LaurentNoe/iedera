#!/bin/sh
# Multinomial tests
echo "4\n        0       1\n                0       1\n                        0        x\n                1       1\n                        1        1 - x\n        1       0\n                0       1\n                        2        1 - x*y\n                1       1\n                        3        x*y\n\n        2       0\n                0       1\n                        2        1 - x\n                1       1\n                        3        x\n\n        3       0\n                0       1\n                        2        1 - y\n                1       1\n                        3        y\n" > _filein_multinomial_automaton_
./iedera_gcov -spaced -BSymbols 01 -n 1 -w 11,11 -s 11,22 -z 1 -m "111011001010010111" -l 20 -pF _filein_multinomial_automaton_ || exit 99
